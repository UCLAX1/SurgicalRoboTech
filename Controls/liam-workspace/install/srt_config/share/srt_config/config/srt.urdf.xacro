<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="srt">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import srt urdf file -->
    <xacro:include filename="$(find moveit_resources_srt_description)/urdf/srt.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="srt.ros2_control.xacro" />


    <xacro:srt_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
